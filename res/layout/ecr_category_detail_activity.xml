<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:textcircle="http://schemas.android.com/apk/res/com.yonyou.am.mob.equipinuseratio"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
      
    android:orientation="vertical" 
    android:background="@color/white">
    <LinearLayout 
		    android:layout_width="match_parent"
		    android:layout_height="130dp"
		    android:orientation="horizontal"
		    android:gravity="center"
		    android:paddingLeft="@dimen/activity_vertical_margin"
		    android:paddingRight="@dimen/activity_vertical_margin"
		    >
		    <com.yonyou.am.mob.tool.DoubleTextCircle
		        android:id="@+id/onetextcircle_l"
		        android:layout_width="wrap_content"
	        	android:layout_height="wrap_content"
		        textcircle:first_text="0%"
		        textcircle:second_text=""
		        textcircle:circle_radius="35dp"
		        textcircle:circle_color="@color/red"
		        textcircle:textsize="17sp"
		        />
		     <TextView
 	        	android:layout_width="20dp"
 	        	android:layout_height="1dp"/>
		    <com.yonyou.am.mob.tool.DoubleTextCircle
		        android:id="@+id/onetextcircle_m"
		        android:layout_width="wrap_content"
	        	android:layout_height="wrap_content"
		        textcircle:first_text="0%"
		        textcircle:second_text=""
		        textcircle:circle_radius="50dp"
		        textcircle:circle_color="@color/red"
		        textcircle:textsize="20sp"
		     />
		     <TextView
   	        	android:layout_width="20dp"
   	        	android:layout_height="1dp"/>
		    <com.yonyou.am.mob.tool.DoubleTextCircle
		        android:id="@+id/onetextcircle_r"
		        android:layout_width="wrap_content"
	        	android:layout_height="wrap_content"
		        textcircle:first_text="0%"
		        textcircle:second_text=""
		        textcircle:circle_radius="35dp"
		        textcircle:circle_color="@color/red"
		        textcircle:textsize="17sp"        
		    />
		</LinearLayout>
		
		<LinearLayout 
		    android:layout_width="match_parent"
		    android:layout_height="30dp"
		    android:orientation="horizontal"
		    android:gravity="center_vertical"
		    android:background="#008B8B"
		    >
		    <TextView 
		        android:id="@+id/goodrate"
		        android:layout_width="0dp"
		        android:layout_height="match_parent"
		        android:layout_weight="1"
		        android:text="@string/goodrate"
		        android:gravity="center"
		        android:textSize="16sp"
		        android:textColor="@color/white"
		        />
		    <TextView
		        android:layout_width="1dp"
		        android:layout_height="25dp"
		        android:background="@color/white"
		        android:layout_gravity="bottom" 
		        />
		     <TextView 
		        android:layout_width="0dp"
		        android:layout_height="match_parent"
		        android:layout_weight="1"
		        android:text="@string/goodequipnum"
		        android:gravity="center"
		        android:textSize="16sp"
		        android:textColor="@color/white"
		        />
		    <TextView
		        android:layout_width="1dp"
		        android:layout_height="25dp"
		        android:background="@color/white" 
		        android:layout_gravity="bottom" 
		        />
		     <TextView 
		        android:layout_width="0dp"
		        android:layout_height="match_parent"
		        android:layout_weight="1"
		        android:text="@string/badequipnum"
		        android:gravity="center"
		        android:textSize="16sp"
		        android:textColor="@color/white"
		        />
		</LinearLayout>
    	<LinearLayout
    	    android:id="@+id/ratiolinearlayout"
		    android:layout_width="match_parent"
		    android:layout_height="50dp"
		    android:orientation="horizontal"
		    android:background="#00C5CD">
    	   <TextView 
    	        android:id="@+id/detailgoodrate"
		        android:layout_width="0dp"
		        android:layout_height="match_parent"
		        android:layout_weight="1"
		        android:text="@string/goodrate"
		        android:gravity="center"
		        android:textSize="16sp"
		        android:textColor="@color/white"
		        />
		    <TextView
		        android:layout_width="1dp"
		        android:layout_height="match_parent"
		        android:background="@color/white"
				android:layout_marginBottom="10dp"
		        />
		     <TextView 
		        android:id="@+id/detailgoodnum"
		        android:layout_width="0dp"
		        android:layout_height="match_parent"
		        android:layout_weight="1"
		        android:text="@string/goodequipnum"
		        android:gravity="center"
		        android:textSize="16sp"
		        android:textColor="@color/white"
		        />
		    <TextView
		        android:layout_width="1dp"
		        android:layout_height="match_parent"
		        android:background="@color/white" 
		        android:layout_marginBottom="10dp"
		        />
		     <TextView
		        android:id="@+id/detailbadnum" 
		        android:layout_width="0dp"
		        android:layout_height="match_parent"
		        android:layout_weight="1"
		        android:text="@string/badequipnum"
		        android:gravity="center"
		        android:textSize="16sp"
		        android:textColor="@color/white"
		        />
		    
		    
		</LinearLayout>
   
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1">
        <LinearLayout 
            android:id="@+id/linearlayoutbar"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            
            
        </LinearLayout>
    </ScrollView>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:background="#00C5CD"
        android:layout_gravity="bottom"
        android:orientation="horizontal"
        >
        <TextView
            android:layout_width="40dp"
            android:layout_height="match_parent"
            android:text="0%"
            android:gravity="center"
            android:layout_marginLeft="100dp"
            android:background="@color/red"
            />
         <TextView
            android:layout_width="40dp"
            android:layout_height="match_parent"
            android:text="20%"
            android:gravity="center"
            />
          <TextView
            android:layout_width="40dp"
            android:layout_height="match_parent"
            android:text="40%"
            android:gravity="center"
            />
           <TextView
            android:layout_width="40dp"
            android:layout_height="match_parent"
            android:text="60%"
            android:gravity="center"
            />
            <TextView
            android:layout_width="40dp"
            android:layout_height="match_parent"
            android:text="80%"
            android:gravity="center"
            />
             <TextView
            android:layout_width="40dp"
            android:layout_height="match_parent"
            android:text="100%"
            android:gravity="center"
            />
        
    </LinearLayout>
    
</LinearLayout>